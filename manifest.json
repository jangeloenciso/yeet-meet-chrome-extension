{
    "manifest_version": 2,
    "name": "Yeet Meet",
    "version": "0.1.0",
    "content_scripts": [
        {
            "matches": ["*://*.meet.google.com/*"],
            "js": ["script.js"],
            "css": ["styles.css"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html"
    }
}
