{
    "manifest_version": 3,
    "name": "Yeet Meet",
    "description": "Yeets you out of a google meet when there are only 4 or less participants left.",
    "version": "0.1.0",
    "content_scripts": [
        {
            "matches": ["*://*.meet.google.com/*"],
            "js": ["script.js"]
        }
    ],
    "action": {
        "default_icon": "./images/icon2.png",
        "default_popup": "popup.html"
    },
    "permissions": ["tabs", "activeTab", "scripting"]
}
